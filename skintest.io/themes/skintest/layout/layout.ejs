<!DOCTYPE html>
<html lang="en">

<head>

  <title><%- theme.title %></title>

  <meta charset="utf-8">
  <meta name="description"
        content="<%- theme.description %>">
  <meta name="viewport"
        content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
  <meta property="og:type"
        content="article">
  <meta property="og:title"
        content="<%- theme.title %>">
  <meta property="og:description"
        content="<%- theme.site_description %>">
  <meta property="og:image"
        content="https://<%- theme.root_domain %>/<%- url_for('/images/logo.png') %>">

  <link rel="apple-touch-icon"
        sizes="57x57"
        href="<%- url_for('/assets/favicons/favicon.svg') %>">
  <link rel="apple-touch-icon"
        sizes="60x60"
        href="<%- url_for('/assets/favicons/favicon.svg') %>">
  <link rel="apple-touch-icon"
        sizes="72x72"
        href="<%- url_for('/assets/favicons/favicon.svg') %>">
  <link rel="apple-touch-icon"
        sizes="76x76"
        href="<%- url_for('/assets/favicons/favicon.svg') %>">
  <link rel="apple-touch-icon"
        sizes="114x114"
        href="<%- url_for('/assets/favicons/favicon.svg') %>">
  <link rel="apple-touch-icon"
        sizes="120x120"
        href="<%- url_for('/assets/favicons/favicon.svg') %>">
  <link rel="apple-touch-icon"
        sizes="144x144"
        href="<%- url_for('/assets/favicons/favicon.svg') %>">
  <link rel="apple-touch-icon"
        sizes="152x152"
        href="<%- url_for('/assets/favicons/favicon.svg') %>">
  <link rel="apple-touch-icon"
        sizes="180x180"
        href="<%- url_for('/assets/favicons/favicon.svg') %>">
  <link rel="icon"
        type="image/png"
        sizes="192x192"
        href="<%- url_for('/assets/favicons/favicon.svg') %>">
  <link rel="icon"
        type="image/png"
        sizes="384x384"
        href="<%- url_for('/assets/favicons/favicon.svg') %>">
  <link rel="icon"
        type="image/png"
        sizes="32x32"
        href="<%- url_for('/assets/favicons/favicon.svg') %>">
  <link rel="icon"
        type="image/png"
        sizes="96x96"
        href="<%- url_for('/assets/favicons/favicon.svg') %>">
  <link rel="icon"
        type="image/png"
        sizes="16x16"
        href="<%- url_for('/assets/favicons/favicon.svg') %>">
  <meta name="msapplication-TileImage"
        content='/assets/favicons/favicon.svg.png'>
  <link rel="icon"
        href="<%- url_for('/assets/logo.png') %>"
        type="image/png">

  <meta name="msapplication-TileColor"
        content="#2f80ed">
  <meta name="theme-color"
        content="#2f80ed">

  <meta name="msapplication-config"
        content="browserconfig.xml">
  <link rel="manifest"
        href="/manifest.json">

  <link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700&display=swap" rel="stylesheet">

  <%- css('css/index')
      %>

    <script src="<%- url_for('/js/scroll.js') %>"></script>
    <script src="<%- url_for('/js/video.js') %>"></script>
    <script>window.PAGE_TYPE = "<%- page.group %>"</script>

</head>

<body>
    <header class="container container-header">
      <%- partial('partials/header')
          %>
    </header>

    <main class="container container-main">
      <%- body
          %>
    </main>

    <footer class="container container-footer">
      <%- partial('partials/footer')
          %>
    </footer>
</body>

</html>