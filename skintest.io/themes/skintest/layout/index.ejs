<section class="helper">
  <h2 class="title title-secondary title-helper">Language that helps to develop</h2>
  <div class="wrapper wrapper-helper">
    <div class="helper__code">

      <pre class="language-typescript">
        <code class="yaml language-typescript">
<span><span class="token function">feature</span>('todos add')</span>

  <span>.<span class="token function">before</span>('scenario'</span>
    <span>, I.<span class="token function">open</span>(page.start)</span>
    <span>, I.<span class="token function">open</span>(todos.url)</span>
    <span>, I.<span class="token function">open</span>(clear_todos)</span>
  <span>)</span>

  <span>.<span class="token function">scenario</span>(‘ check the list has all added items’</span>
    <span>, I.<span class="token function">do</span>(add_todo, ‘learn testing’)</span>
    <span>, I.<span class="token function">do</span>(add_todo, ‘be cool’)</span>
    <span>, I.<span class="token function">test</span>(‘list contains added items’)</span>
    <span>, I.<span class="token function">see</span>(todos.list, has.lenght, <span class="token number">2</span>)</span>
    <span>, I.<span class="token function">see</span>(todos.<span class="token function">item_label_at</span>(<span class="token number">0</span>), has.text, ‘learn testing’)</span>
    <span>, I.<span class="token function">see</span>(todos.<span class="token function">item_label_at</span>(<span class="token number">1</span>), has.text, ‘be cool’)</span>
  <span>)</span>
        </code>
      </pre>
    </div>
    <ul class="helper__list">
      <li class="helper__point helper__point-execution"><span>Transparent</span></li>
      <li class="helper__point helper__point-safty"><span>Type safety</span></li>
      <li class="helper__point helper__point-api"><span>Unified APIs</span></li>
    </ul>
  </div>
</section>

