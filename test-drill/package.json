{
  "name": "@skintest/drill",
  "version": "1.0.19-alpha.0",
  "private": true,
  "description": "skintest exercise",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "yarn clean && tsc --build",
    "build:watch": "yarn clean && tsc --build --watch",
