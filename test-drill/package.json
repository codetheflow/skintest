  },
  "homepage": "https://github.com/codetheflow/skintest#readme",
  "devDependencies": {
    "@types/node": "16.4.1",
    "nodemon": "^2.0.12",
    "rimraf": "3.0.2",
    "typescript": "4.3.5"
  },
  "peerDependencies": {
    "@skintest/common": "*",
    "@skintest/enterprise": "*",
    "@skintest/platform": "*",
