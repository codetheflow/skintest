{
  "name": "skintest",
  "description": "e2e automation tool",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/assets/get_started16.png",
      "32": "/assets/get_started32.png",
      "48": "/assets/get_started48.png",
      "128": "/assets/get_started128.png"
    }
  },
  "icons": {
    "16": "/assets/get_started16.png",
    "32": "/assets/get_started32.png",
    "48": "/assets/get_started48.png",
    "128": "/assets/get_started128.png"
  }
}